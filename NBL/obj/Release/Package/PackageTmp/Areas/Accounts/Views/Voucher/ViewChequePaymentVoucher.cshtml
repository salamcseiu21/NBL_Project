@using NBL.Areas.Accounts.Models
@model IEnumerable<Voucher>
@{
    ViewBag.Title = "Cheque Payment Voucher";
    Layout = "~/Views/Shared/_UserLayoutPage.cshtml";
}

<section class="content">
    <h3 class="text-green text-center headingStyle">Cheque Payment Voucher</h3>

    <h3 class="text-green"><label> Total Cheque Payment : @Model.Sum(n => n.Amounts) (&#2547;)</label></h3>
    @Html.Partial("_VoucherPartialPage", Model)

</section>
